            %a{:href => "#} 9989964369
        .jquery-script-ads
          :javascript
                      %script{:src => "http://pagead2.googlesyndication.com/pagead/show_ads.js", :type => "text/javascript"}
        .jquery-script-clear
    %h1{:align => "center", :style => "margin-top:150px; margin-bottom:30px"} Lot Mobiles
    #myCarousel.carousel.slide{"data-ride" => "carousel"}
      / Indicators
      %ol.carousel-indicators
        %li.active{"data-slide-to" => "0", "data-target" => "#myCarousel"}
        %li{"data-slide-to" => "1", "data-target" => "#myCarousel"}
        %li{"data-slide-to" => "2", "data-target" => "#myCarousel"}
      .carousel-inner
        .item.active
          %img{:alt => "First slide", :src => "http://lorempixel.com/1200/400/sports", :style => "width:100%"}
            .container
              .carousel-caption
                %h1 i love u
                %p i dont have any words to say it
               
                 
        .item
          %img{:alt => "Second    slide", "data-src" => "", :src => "http://lorempixel.com/1200/400/people", :style => "width:100%"}
            .container
              .carousel-caption
                %h1 Miss u
                %p Never minded this this distance until i felt this feel
               
        .item
          %img{:alt => "Third slide", "data-src" => "", :src => "http://lorempixel.com/1200/400/abstract", :style => "width:100%"}
            .container
              .carousel-caption
                %h1  
                %p 
                
      %a.left.carousel-control{"data-slide" => "prev", :href => "#myCarousel"}
        %span.glyphicon.glyphicon-chevron-left
      %a.right.carousel-control{"data-slide" => "next", :href => "#myCarousel"}
        %span.glyphicon.glyphicon-chevron-right
