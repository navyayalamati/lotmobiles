<div id="block-block-4" class="block block-block block-2">
    <div class="content"><script text="text/javascript">
	$.fn.inOrder = function(sel) {
		return this.filter(function() {
			return $(this).is(sel);
		});
	}

	var dom = "<div id='toc_content'><a name='toc'></a><h4>Contents <span class='toggle_button'>[<span class='hide_button'>Hide</span><span class='show_button'>Show</span>]</span></h4>";
	var counter = 0;
	var level = 0;
	var sublevels = [0, 0, 0, 0, 0, 0, 0, 0];
	var page_path = location.protocol + "//" + location.hostname + location.pathname;
	var headings = $("#content *").inOrder("h2,h3,h4,h5,h6,h7,h8,h9");
		
	if(location.pathname != '/kb'  && location.pathname != '/kb/' && $(headings)[1]) {
		$(headings).each(function() {
			if(this.nodeName.substring(1) > level) {
				level = this.nodeName.substring(1);
				sublevels[level-2] = 1;
				dom += "<ol>";
			}
			else if(this.nodeName.substring(1) < level) {
				sublevels[level-2] = 0;
				level = this.nodeName.substring(1);
				sublevels[level-2]++;
				dom += "</ol>";
			} else {
				sublevels[level-2]++;
			}
			
			dom += "<li>";
			dom += "<span>";
			for(i = 2; i<=level; i++)
				dom += sublevels[i-2] + ".";
			dom += "</span> <a href='" + page_path + "#heading_" + counter + "'>" + $(this).text() + "</a></li>";
		
			$(this).prepend("<a name='heading_" + counter + "'></a>");
			$(this).append("<a class='top_link' href='" + page_path + "#toc'>Top</a>");
			counter++;
		});
	
		for(i = level; i>1; i--)
			dom += "</ol>";
		dom += "</div>";

		$("#toc").html(dom);

		$('#toc .toggle_button').click(function() {
			$('#toc_content>ol').animate({"height": "toggle", "opacity": "toggle"}, "slow");
			$('#toc .toggle_button span').toggle();
		});
	}

	$(function(){
	    $('a[href*=#]').click(function() {
	    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'')
	        && location.hostname == this.hostname) {
	            var $target = $(this.hash);
	            $target = $target.length && $target || $('[name=' + this.hash.slice(1) +']');
	            if ($target.length) {
	                var targetOffset = $target.offset().top;
	                $('html,body').animate({scrollTop: targetOffset}, 1000);
	                return false;
	            }
	        }
	    });
	});
</script>

</div>
</div>
    </div>
      </div></div><!-- end center -->
